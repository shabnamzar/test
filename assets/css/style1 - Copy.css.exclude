.btn-scroll-top {
    color: #fff;
    border-radius: 50%;
    cursor: pointer;
    font-size: 25px;
    display: block;
    text-align: center;
    line-height: 56px;
    position: fixed;
    bottom: 100px;
    right: 60px;
    height: 50px;
    width: 50px;
    z-index: 999;
    background-color: #33245f;
    opacity: 0;
    filter: alpha(opacity=0);
    visibility: hidden;
    -webkit-transition: .4s;
    -o-transition: .4s;
    transition: .4s;
    -webkit-transform: scale(0);
    -moz-transform: scale(0);
    -ms-transform: scale(0);
