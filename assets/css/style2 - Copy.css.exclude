.CustomCheckbox {
    display: inline-block;
    background-color: rgba(255, 255, 255, .9);
    color: #adadad;
    border-radius: 10px;
    white-space: nowrap;
    margin: -5px 0px 14px 10px;
    -webkit-touch-callout: none;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    -webkit-tap-highlight-color: transparent;
    /* transition: all .2s; */
    padding: 12px;
    float: right;
    cursor: pointer;
    -webkit-box-shadow: 0 5px 5px 0 rgba(0,0,0,.2);
    -moz-box-shadow: 0 5px 5px 0 rgba(0,0,0,.2);
    box-shadow: 0 2px 8px 0 rgba(0,0,0,.2);
}


    .CustomCheckbox::before {
        display: inline-block;
        font-style: normal;
        font-variant: normal;
        text-rendering: auto;
        -webkit-font-smoothing: antialiased;
        font-weight: 900;
        font-size: 20px;
        padding: 2px 4px;
        transition: transform .3s ease-in-out;
    }

.CustomCheckbox-container input[type="checkbox"] {
    position: absolute;
    opacity: 0;
}

.CustomCheckbox::before {
    content: "";
}


.CustomCheckbox-container input[type="checkbox"]:checked + label::before {
    content: "\f00c";
    transform: rotate(-360deg);
    transition: transform .3s;
}

.CustomCheckbox-container input[type="checkbox"]:checked + label {
    padding: 2.5px 2px;
    transform: scale(1.1, 1.1);
}

.CustomCheckbox-container .CustomCheckboxtext {
    font-size: 14px;
    font-weight: 600;
    line-height: 25px;
